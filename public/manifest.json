{
    "manifest_version": 3,
    "name": "Audio Console",
    "version": "1.0",

    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content_script.js"]
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}
